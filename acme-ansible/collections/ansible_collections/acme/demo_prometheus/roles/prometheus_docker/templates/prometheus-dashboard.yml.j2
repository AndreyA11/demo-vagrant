# {{ ansible_managed }}
apiVersion: 1

providers:
  - name: 'prometheus'
    orgId: 1
    folder: ''
    folderUid: ''
    type: file
    disableDeletion: true
    updateIntervalSeconds: 3600
    allowUiUpdates: false
    options:
      path: /etc/grafana/provisioning/dashboards/prometheus-dashboard/
      foldersFromFilesStructure: false